{
    "name": "amethyst/core",
    "keywords": ["laravel"],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "railken/bag": "^1.2",
        "railken/eloquent-mapper": "0.3.*",
        "laravel/framework": "5.8.* || 6.*",
        "league/fractal": "^0.17.0",
        "railken/lem": "^3.3",
        "railken/cacheable": "0.1.*",
        "doctrine/inflector": "^1.3",
        "hanneskod/classtools": "^1.2",
        "railken/eloquent-instance": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "orchestra/testbench": "*"
    },
    "autoload": {
        "psr-4" : {
            "Amethyst\\Core\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Amethyst\\Core\\Tests\\": "tests/" 
        }
    }
}
